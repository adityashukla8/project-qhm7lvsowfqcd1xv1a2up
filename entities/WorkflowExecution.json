{
  "name": "WorkflowExecution",
  "description": "Track agentic workflow executions and their status",
  "properties": {
    "workflow_id": {
      "type": "string",
      "description": "Unique workflow execution identifier"
    },
    "patient_id": {
      "type": "string",
      "description": "Associated patient ID"
    },
    "workflow_type": {
      "type": "string",
      "description": "Type of workflow: trial_matching, enrichment, analysis"
    },
    "status": {
      "type": "string",
      "description": "Workflow status: pending, running, completed, failed"
    },
    "progress": {
      "type": "number",
      "description": "Workflow progress percentage (0-100)"
    },
    "current_step": {
      "type": "string",
      "description": "Current workflow step description"
    },
    "results": {
      "type": "string",
      "description": "JSON string of workflow results"
    },
    "error_message": {
      "type": "string",
      "description": "Error message if workflow failed"
    },
    "started_at": {
      "type": "string",
      "description": "When the workflow started"
    },
    "completed_at": {
      "type": "string",
      "description": "When the workflow completed"
    }
  }
}